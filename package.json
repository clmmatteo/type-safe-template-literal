{
  "name": "@clmmatteo/type-safe-template-literal",
  "version": "1.0.0",
  "description": "A simple tagged template that allows to use stricter, type-safe template literals in TypeScript.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && sh ./fixup",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clmmatteo/type-safe-template-literal.git"
  },
  "type": "module",
  "keywords": [
    "typescript",
    "template literals",
    "tagged templates",
    "typesafe",
    "type-safe"
  ],
  "author": "Matteo Colombo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clmmatteo/type-safe-template-literal/issues"
  },
  "homepage": "https://github.com/clmmatteo/type-safe-template-literal#readme",
  "devDependencies": {
    "@types/node": "^18.6.3",
    "typescript": "^4.7.4"
  }
}
